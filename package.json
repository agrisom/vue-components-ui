  "unpkg": "dist/vue-components-ui.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": [
    "*.vue"
  ],
  "scripts": {
    "serve": "start-storybook -p 6006",
